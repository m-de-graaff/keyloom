{
  "name": "@keyloom/adapters",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./prisma": {
      "types": "./dist/prisma/index.d.ts",
      "import": "./dist/prisma/index.js",
      "require": "./dist/prisma/index.cjs"
    },
    "./drizzle-postgres": {
      "types": "./dist/drizzle-postgres/index.d.ts",
      "import": "./dist/drizzle-postgres/index.js",
      "require": "./dist/drizzle-postgres/index.cjs"
    },
    "./drizzle-mysql": {
      "types": "./dist/drizzle-mysql/index.d.ts",
      "import": "./dist/drizzle-mysql/index.js",
      "require": "./dist/drizzle-mysql/index.cjs"
    },
    "./mongo": {
      "types": "./dist/mongo/index.d.ts",
      "import": "./dist/mongo/index.js",
      "require": "./dist/mongo/index.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "prisma": [
        "dist/prisma/index.d.ts"
      ],
      "drizzle-postgres": [
        "dist/drizzle-postgres/index.d.ts"
      ],
      "drizzle-mysql": [
        "dist/drizzle-mysql/index.d.ts"
      ],
      "mongo": [
        "dist/mongo/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "prisma"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "typecheck": "tsc -b --pretty false",
    "test": "vitest run"
  },
  "peerDependencies": {
    "@keyloom/core": "workspace:*"
  }
}
