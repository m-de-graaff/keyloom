{
  "name": "@keyloom/adapters-mongo",
  "version": "0.1.0",
  "description": "MongoDB adapter for Keyloom (skeleton)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && pnpm build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist --skipLibCheck",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "test": "vitest run --passWithNoTests",
    "lint": "biome check ."
  },
  "dependencies": {
    "@keyloom/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "workspace:^3.2.4"
  },
  "peerDependencies": {
    "typescript": ">=5.4"
  }
}
