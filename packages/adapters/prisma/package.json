{
  "name": "@keyloom/adapters-prisma",
  "version": "1.4.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./jwt": {
      "types": "./dist/jwt.d.ts",
      "import": "./dist/jwt.js",
      "require": "./dist/jwt.cjs"
    }
  },
  "files": [
    "dist",
    "prisma"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "test": "vitest run",
    "lint": "biome check .",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@keyloom/core": "workspace:*"
  },
  "devDependencies": {
    "@prisma/client": "^6.16.1",
    "prisma": "^6.16.1",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "@prisma/client": "^5.18.0",
    "typescript": ">=5.4"
  }
}
