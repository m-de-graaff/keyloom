{
  "name": "@keyloom/server",
  "version": "2.1.7",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && pnpm build:types",
    "build:types": "tsc -b --declaration --emitDeclarationOnly",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "test": "vitest run --passWithNoTests",
    "lint": "biome check ."
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": ">=5.4"
  },
  "dependencies": {
    "@keyloom/adapters": "latest",
    "@keyloom/core": "latest",
    "@node-rs/argon2": "^2.0.2",
    "@prisma/client": "^6.16.1",
    "bcryptjs": "^3.0.2",
    "fastify": "^5.6.0",
    "zod": "^4.1.8"
  }
}
