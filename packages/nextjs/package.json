{
  "name": "@keyloom/nextjs",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "development": "./src/index.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "./edge": {
      "types": "./dist/edge.d.ts",
      "import": {
        "development": "./src/edge.ts",
        "default": "./dist/edge.js"
      }
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": {
        "development": "./src/middleware.ts",
        "default": "./dist/middleware.js"
      }
    },
    "./handler": {
      "types": "./dist/handler.d.ts",
      "import": {
        "development": "./src/handler.ts",
        "default": "./dist/handler.js"
      }
    },
    "./rsc": {
      "types": "./dist/rsc.d.ts",
      "import": {
        "development": "./src/rsc.ts",
        "default": "./dist/rsc.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "test": "vitest run",
    "lint": "biome check ."
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": ">=5.4"
  }
}

