{
  "name": "@keyloom/nextjs",
  "version": "1.4.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",

  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./middleware": {
      "import": "./dist/middleware.js",
      "require": "./dist/middleware.cjs"
    },
    "./edge": {
      "import": "./dist/edge.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "test": "vitest run",
    "lint": "biome check ."
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "next": ">=13",
    "react": ">=18",
    "typescript": ">=5.4",
    "@keyloom/core": "workspace:*"
  }
}
