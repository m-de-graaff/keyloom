{
  "name": "@keyloom/nextjs",
  "version": "3.0.3",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.js",
      "require": "./dist/middleware.cjs"
    },
    "./edge": {
      "types": "./dist/edge.d.ts",
      "import": "./dist/edge.js"
    },
    "./route-types": {
      "types": "./dist/route-types.d.ts",
      "import": "./dist/route-types.js",
      "require": "./dist/route-types.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && pnpm build:types",
    "build:types": "tsc -b --force",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "test": "vitest run",
    "lint": "biome check ."
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "@keyloom/core": "workspace:*"
  },
  "dependencies": {},
  "peerDependencies": {
    "@keyloom/core": ">=2.1.7",
    "next": ">=13",
    "react": ">=18",
    "typescript": ">=5.4"
  }
}
