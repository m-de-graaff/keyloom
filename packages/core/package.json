    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && pnpm build:types",
    "build:types": "tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "test": "vitest run",
    "lint": "biome check ."
  },
  "devDependencies": {
    "@types/node": "^24.5.0",
