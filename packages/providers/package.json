{
  "name": "@keyloom/providers",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./github": {
      "types": "./dist/github/index.d.ts",
      "import": "./dist/github/index.js",
      "require": "./dist/github/index.cjs"
    },
    "./google": {
      "types": "./dist/google/index.d.ts",
      "import": "./dist/google/index.js",
      "require": "./dist/google/index.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "github": [
        "dist/github/index.d.ts"
      ],
      "google": [
        "dist/google/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "typecheck": "tsc -b --pretty false",
    "test": "vitest run"
  },
  "peerDependencies": {
    "@keyloom/core": "workspace:*"
  }
}
