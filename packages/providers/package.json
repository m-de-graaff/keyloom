{
  "name": "@keyloom/providers",
  "version": "1.4.4",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./github": {
      "types": "./dist/github.d.ts",
      "import": "./dist/github.js",
      "require": "./dist/github.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "test": "vitest run",
    "lint": "biome check ."
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": ">=5.4"
  },
  "license": "MIT",
  "author": "Keyloom Contributors",
  "description": "OAuth providers for Keyloom (GitHub, etc.)",
  "keywords": ["keyloom", "auth", "oauth", "providers"]
}
