{
  "name": "@keyloom/providers",
  "version": "2.1.5",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./github": {
      "types": "./dist/github.d.ts",
      "import": "./dist/github.js",
      "require": "./dist/github.cjs"
    },
    "./google": {
      "types": "./dist/google.d.ts",
      "import": "./dist/google.js",
      "require": "./dist/google.cjs"
    },
    "./discord": {
      "types": "./dist/discord.d.ts",
      "import": "./dist/discord.js",
      "require": "./dist/discord.cjs"
    },
    "./x": {
      "types": "./dist/x.d.ts",
      "import": "./dist/x.js",
      "require": "./dist/x.cjs"
    },
    "./gitlab": {
      "types": "./dist/gitlab.d.ts",
      "import": "./dist/gitlab.js",
      "require": "./dist/gitlab.cjs"
    },
    "./microsoft": {
      "types": "./dist/microsoft.d.ts",
      "import": "./dist/microsoft.js",
      "require": "./dist/microsoft.cjs"
    },
    "./apple": {
      "types": "./dist/apple.d.ts",
      "import": "./dist/apple.js",
      "require": "./dist/apple.cjs"
    },
    "./auth0": {
      "types": "./dist/auth0.d.ts",
      "import": "./dist/auth0.js",
      "require": "./dist/auth0.cjs"
    },
    "./dev": {
      "types": "./dist/dev.d.ts",
      "import": "./dist/dev.js",
      "require": "./dist/dev.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && pnpm build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist --skipLibCheck",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "test": "vitest run --passWithNoTests",
    "lint": "biome check ."
  },
  "devDependencies": {
    "@keyloom/core": "workspace:*",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": ">=5.4"
  },
  "license": "MIT",
  "author": "Keyloom Contributors",
  "description": "OAuth providers for Keyloom (GitHub, etc.)",
  "keywords": [
    "keyloom",
    "auth",
    "oauth",
    "providers"
  ]
}
